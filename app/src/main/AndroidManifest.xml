<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aidana.geotracker" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".GeoActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateAccountActivity"
            android:label="@string/title_activity_create_account" >
        </activity>
        <activity
            android:name=".AccountActivity"
            android:label="@string/account_activity" >
        </activity>
        <activity
            android:name=".NewPasswordActivity"
            android:label="@string/reset_password_activity" >
        </activity>
        <activity
            android:name=".SecurityQuestionActivity"
            android:label="@string/security_question_activity" >
        </activity>
    </application>

</manifest>
